

<form action="/admin/category/update"autocomplete="off" method="POST">
    <input type="hidden" name="_id" value="<%=category._id%>">
    <div class="form-group">
        <label>Name</label>
        <input type="text" class="form-control" name="name" placeholder="Name"value="<%= category.name %>" >
        <!-- <div class="text-danger">
            {{category.fullNameError}}</div> -->
    </div>
    <div class="form-group">
        <label>Keywords</label>
        <input type="text" class="form-control" name="cat_keywords" placeholder="Keywords"value="<%= category.cat_keywords %>">
        <!-- <div class="text-danger">
            {{category.emailError}}</div>
    </div> -->

  
    <div class="form-row">
        <div class="form-group col-md-6">
            <label>category_type</label>
            <select class="form-control" name="cat_type" tabindex="-1" aria-hidden="true">
                <option value="static">Static</option>
                <option value="animated">Animated</option>
            </select>        
        </div>
        <div class="form-group col-md-6">
            <label>Featured_type</label>
            <select class="form-control" name="featured_type" tabindex="-1" aria-hidden="true">
                <option value="static">Static</option>
                <option value="animated">Animated</option>
            </select>        
        </div>
    </div>
    <div class="form-group">
        <label>Image</label>
        <input type="file" class="form-control" name="files"value="<%= category.files %>>
     
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-info"><i class="fa fa-database"></i> Submit</button>
    </div>
</form>