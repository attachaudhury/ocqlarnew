


<form action="/admin/registration" method="POST" autocomplete="on" enctype="multipart/form-data">
    <div class="form-group">
        <label>First Name</label>
        <input type="text" class="form-control" name="fname" placeholder="First Name" >
       
    </div>
    <div class="form-group">
        <label>Last Name</label>
        <input type="text" class="form-control" name="lname" placeholder=" Last Name" >
        
    </div>
  

  
    <div class="form-row">
        <div class="form-group col-md-6">
            <label>* Select Your Package</label>
            <select class="form-control" name="package" tabindex="-1" aria-hidden="true">
                <option value="basic">Basic</option>
                <option value="delux">Delux</option>
            </select>        
        </div>
        <div class="form-group">
            <label>Email</label>
            <input type="email" class="form-control" name="email" placeholder=" Email"autocomplete="email" >
            
        </div>
    
    </div>
    <div class="form-group">
        <label>Password</label>
        <input type="password" class="form-control" name="password" placeholder=" Password" autocomplete="password">
        
    </div>
    <div class="form-group">
        <label>image</label>
        <input type="file" class="form-control" name="files">
        
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-info"><i class="fa fa-database"></i> Submit</button>
      
    </div>
</form>